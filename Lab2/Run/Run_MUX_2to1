#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x556f5e18bdb0 .scope module, "testbench" "testbench" 2 3;
 .timescale -9 -12;
v0x556f5e19eca0_0 .net "result", 0 0, v0x556f5e1517b0_0;  1 drivers
v0x556f5e19ed60_0 .var "select", 0 0;
v0x556f5e19ee30_0 .var "src1", 0 0;
v0x556f5e19ef30_0 .var "src2", 0 0;
S_0x556f5e18bf40 .scope module, "uut" "MUX_2to1" 2 9, 3 4 0, S_0x556f5e18bdb0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "src1";
    .port_info 1 /INPUT 1 "src2";
    .port_info 2 /INPUT 1 "select";
    .port_info 3 /OUTPUT 1 "result";
v0x556f5e1517b0_0 .var "result", 0 0;
v0x556f5e151ba0_0 .net "select", 0 0, v0x556f5e19ed60_0;  1 drivers
v0x556f5e19ea60_0 .net "src1", 0 0, v0x556f5e19ee30_0;  1 drivers
v0x556f5e19eb30_0 .net "src2", 0 0, v0x556f5e19ef30_0;  1 drivers
E_0x556f5e18c3d0 .event edge, v0x556f5e151ba0_0, v0x556f5e19ea60_0, v0x556f5e19eb30_0;
    .scope S_0x556f5e18bf40;
T_0 ;
    %wait E_0x556f5e18c3d0;
    %load/vec4 v0x556f5e151ba0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 1;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 1;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %jmp T_0.2;
T_0.0 ;
    %load/vec4 v0x556f5e19ea60_0;
    %store/vec4 v0x556f5e1517b0_0, 0, 1;
    %jmp T_0.2;
T_0.1 ;
    %load/vec4 v0x556f5e19eb30_0;
    %store/vec4 v0x556f5e1517b0_0, 0, 1;
    %jmp T_0.2;
T_0.2 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x556f5e18bdb0;
T_1 ;
    %vpi_call 2 17 "$display", "select src1 src2 | result" {0 0 0};
    %vpi_call 2 18 "$display", "-------------------------" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556f5e19ee30_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556f5e19ef30_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556f5e19ed60_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 21 "$display", "   %b      %b     %b  |   %b", v0x556f5e19ed60_0, v0x556f5e19ee30_0, v0x556f5e19ef30_0, v0x556f5e19eca0_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556f5e19ee30_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556f5e19ef30_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556f5e19ed60_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 24 "$display", "   %b      %b     %b  |   %b", v0x556f5e19ed60_0, v0x556f5e19ee30_0, v0x556f5e19ef30_0, v0x556f5e19eca0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556f5e19ee30_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556f5e19ef30_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556f5e19ed60_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 27 "$display", "   %b      %b     %b  |   %b", v0x556f5e19ed60_0, v0x556f5e19ee30_0, v0x556f5e19ef30_0, v0x556f5e19eca0_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556f5e19ee30_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556f5e19ef30_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556f5e19ed60_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 30 "$display", "   %b      %b     %b  |   %b", v0x556f5e19ed60_0, v0x556f5e19ee30_0, v0x556f5e19ef30_0, v0x556f5e19eca0_0 {0 0 0};
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_1;
